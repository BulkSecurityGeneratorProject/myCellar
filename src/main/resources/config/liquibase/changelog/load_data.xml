<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="load_data" author="jhipster">

        <loadData encoding="UTF-8" file="config/liquibase/countries.csv"
                  separator=";" tableName="COUNTRY"/>

        <loadData encoding="UTF-8" file="config/liquibase/appellations.csv"
                  separator=";" tableName="APPELLATION"/>

        <loadData encoding="UTF-8" file="config/liquibase/domains.csv"
