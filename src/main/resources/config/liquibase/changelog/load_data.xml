        <loadData encoding="UTF-8" file="config/liquibase/vinewards.csv"
                  separator=";" tableName="VINEWARD"/>

        <loadData encoding="UTF-8" file="config/liquibase/categories.csv"
                  separator=";" tableName="CATEGORY"/>

        <loadData encoding="UTF-8" file="config/liquibase/bottles.csv"
                  separator=";" tableName="BOTTLE"/>

        <loadData encoding="UTF-8" file="config/liquibase/bottle_lifes.csv"
                  separator=";" tableName="BOTTLELIFE"/>

    </changeSet>

</databaseChangeLog>
